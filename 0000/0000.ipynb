    "# %load 0000.py\n",
    "from PIL import Image, ImageDraw, ImageFont\n",
    "front = ImageFont.truetype(\"arial.ttf\", 15)\n",
    "im = Image.open(\"image.jpg\")\n",
    "draw = ImageDraw.Draw(im)\n",
    "draw.text((10, 10), \"4\", font=front, fill=(0, 0, 0))\n",
    "im.save(\"change.jpg\")"
